sig File {
    link : set File
}

sig Trash in File {}

sig Protected in File {}

pred inv1 {}   // The trash is empty.
pred inv2 {}   // All files are deleted.
pred inv3 {}   // Some file is deleted.
pred inv4 {}   // Protected files cannot be deleted.
pred inv5 {}   // All unprotected files are deleted.
pred inv6 {}   // A file links to at most one file.
pred inv7 {}   // There is no deleted link.
pred inv8 {}   // There are no links.
pred inv9 {}   // A link does not link to another link.
pred inv10 {}  // If a link is deleted, so is the file it links to.

